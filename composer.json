{
    "name": "ttskch/pagination-service-provider",
    "description": "pagination service provider for the Silex microframework.",
    "keywords": ["Silex", "pagination", "paginator", "pager"],
    "homepage": "https://github.com/ttskch/PaginationServiceProvider",
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Takashi Kanemoto",
            "email": "kanemoto.takashi@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "silex/silex": "~2.0",
        "twig/twig": "~1.27|~2.0",
        "symfony/twig-bridge": "~2.8|~3.0",
        "symfony/translation": "~2.8|~3.0",
        "knplabs/knp-paginator-bundle": "~2.5",
        "cakephp/utility": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "twbs/bootstrap": "3.2.*@dev",
        "fortawesome/font-awesome": "4.0.*@dev",
        "doctrine/dbal": "~2.2"
    },
    "autoload":{
        "psr-4":{
            "Ttskch\\Silex\\": "src/"
        }
    },
    "scripts": {
        "demo": [
            "echo 'Started web server on http://localhost:8888'",
            "php -S localhost:8888 -t demo"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
